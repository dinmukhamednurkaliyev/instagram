# =========================================================
# DART AND FLUTTER CORE FRAMEWORK FILES
# =========================================================
# Core tool directories and compilation artifacts
**/.dart_tool/
**/.pub/
**/.pub-cache/
**/.packages
**/.packages.generated

# Flutter plugins and dependencies tracking
**/.flutter-plugins
**/.flutter-plugins-dependencies

# Application build output directory
**/build/

# Documentation generated by the dartdoc tool
**/doc/api/

# Helper files for the Dart analyzer
**/flutter_*.png
**/linked_*.ds
**/unlinked.ds
**/unlinked_spec.ds

# Flutter Version Management configuration directory
**/.fvm/

# Generated plugin registrant file
**/lib/generated_plugin_registrant.dart

# =========================================================
# AUTOMATICALLY GENERATED CODE FILES
# =========================================================
# Files created by build_runner, Freezed, JSON Serializable, etc.
**/*.g.dart
**/*.gr.dart
**/*.freezed.dart
**/*.config.dart
**/*.inject.dart
**/*.inject.summary
**/*.inject.g.dart
**/*.gen.dart
**/*.chopper.dart
**/*.graphql.dart
**/*.reflectable.dart

# Localization generated files
**/lib/l10n/gen/

# =========================================================
# SECURITY AND SECRETS (STRICT)
# =========================================================
# Never commit environment configuration files
**/.env
**/.env.*
**/.env.development
**/.env.production

# Local configuration properties containing sensitive paths or keys
**/local.properties
**/key.properties

# Dart files explicitly marked as local or secret
**/*.local.dart
**/*.secret.dart

# Keystores and signing certificates
*.jks
*.keystore
*.p12
*.pem
*.key

# =========================================================
# THIRD PARTY SERVICES AND CONFIGURATION
# =========================================================
# Firebase and Google Services configuration files
**/google-services.json
**/GoogleService-Info.plist
**/.firebase/
**/firebase.json
**/.firebaserc

# Amplify and other cloud provider configurations
**/amplify/
**/aws-exports.js
**/awsconfiguration.json

# =========================================================
# INTEGRATED DEVELOPMENT ENVIRONMENTS (IDE)
# =========================================================
# IntelliJ IDEA and Android Studio project files
**/.idea/
**/*.iml
**/*.ipr
**/*.iws
**/.idea/runConfigurations/

# Visual Studio Code project files
#**/.vscode/
.vscode/*
# Exception: Keep shared configurations (see Exceptions section below)

# Eclipse project files
**/.project
**/.classpath
**/.settings/

# Atom editor configuration
.atom/

# =========================================================
# OPERATING SYSTEM GENERATED FILES
# =========================================================
# macOS folder attributes
**/.DS_Store
**/.apple

# Windows thumbnail cache
**/Thumbs.db
**/ehthumbs.db

# Temporary editor files and swap files
**/*.swp
**/*~

# Python compiled files (if using scripts)
*.pyc

# Subversion directory (if present)
.svn/

# =========================================================
# TEMPORARY FILES, LOGS, AND CACHES
# =========================================================
# Log files
**/*.log

# Temporary files
**/*.tmp
**/temp/
**/tmp/

# Build logs
**/.buildlog/

# Local history
**/.history

# Migration working directories
**/migrate_working_dir/

# Source maps for web builds
**/app.*.map.json

# Testing coverage reports
**/coverage/
**/*.lcov

# =========================================================
# ANDROID PLATFORM SPECIFICS
# =========================================================
# Gradle build artifacts and caches
**/android/.gradle
**/android/app/build
**/android/captures

# Local configuration properties
**/android/local.properties

# Generated Plugin Registrant
**/android/**/GeneratedPluginRegistrant.java

# Build variants output directories
**/android/app/debug
**/android/app/profile
**/android/app/release

# Gradle Wrapper (Note: often committed, but ignored per request strictness)
**/android/gradlew
**/android/gradlew.bat
**/android/**/gradle-wrapper.jar

# =========================================================
# IOS PLATFORM SPECIFICS
# =========================================================
# CocoaPods dependencies and artifacts
**/ios/Pods/
**/ios/.symlinks/

# Flutter framework links
**/ios/Flutter/Flutter.framework
**/ios/Flutter/App.framework
**/ios/Flutter/Flutter.podspec

# Generated configuration and build files
**/ios/.generated/
**/ios/Flutter/.last_build_id
**/ios/Flutter/Generated.xcconfig
**/ios/Flutter/app.flx
**/ios/Flutter/app.zip
**/ios/Flutter/flutter_assets/
**/ios/Flutter/flutter_export_environment.sh
**/ios/ServiceDefinitions.json
**/ios/Runner/GeneratedPluginRegistrant.*

# Xcode user-specific data
**/ios/**/xcuserdata
**/ios/**/DerivedData/
**/ios/**/.vagrant/
**/ios/**/.sconsign.dblite
**/ios/**/.tags*

# Xcode moved or backup files
**/ios/**/*.mode1v3
**/ios/**/*.mode2v3
**/ios/**/*.moved-aside
**/ios/**/*.pbxuser
**/ios/**/*.perspectivev3

# =========================================================
# DESKTOP PLATFORMS (WINDOWS, LINUX, MACOS)
# =========================================================
# Ephemeral build files for desktop
**/windows/flutter/ephemeral/
**/linux/flutter/ephemeral/
**/macos/Flutter/ephemeral/

# =========================================================
# COMPILED BINARIES AND LIBRARIES
# =========================================================
**/*.exe
**/*.exe~
**/*.dll
**/*.so
**/*.dylib
**/*.class
app.*.symbols

# =========================================================
# EXCEPTIONS
# =========================================================
# Keep specific Xcode default schemes if needed
!**/ios/**/default.mode1v3
!**/ios/**/default.mode2v3
!**/ios/**/default.pbxuser
!**/ios/**/default.perspectivev3

# Keep dependency configurations for testing
!**/packages/flutter_tools/test/data/dart_dependencies_test/**/.packages

# Keep CI configuration files
!**/dev/ci/**/Gemfile.lock

# Keep useful Visual Studio Code team configurations
!**/.vscode/extensions.json
!**/.vscode/launch.json
!**/.vscode/settings.json
!**/.vscode/tasks.json

# Keep shared IntelliJ code styles and dictionary configurations
!**/.idea/codeStyles/
!**/.idea/dictionaries/
